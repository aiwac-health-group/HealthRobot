<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aiwac.admin.com.healthrobot">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--skin-->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--skin-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--skin activities-->
        <activity android:name=".activity.BeautyResultActivity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.CameraActivity" />
        <activity android:name=".activity.TestHelpActivity" />
        <activity android:name=".activity.DetectProcessActivity"/>
        <activity android:name=".activity.SkinMainActivity"/>
    </application>

</manifest>